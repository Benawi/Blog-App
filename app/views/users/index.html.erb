<%= render "shared/header" %>
<% @users.each do |user| %>
    <div class="user-container">
      <img src="<%= user.photo %>" alt="user photo">
      <div>
        <h2>Name: <%= user.name %></h2>
        
        <h4>
          <%= link_to "View profile", user_path(user) %>
        </h4>
      </div>
    </div>
<% end %>
<%= render "shared/footer" %>
